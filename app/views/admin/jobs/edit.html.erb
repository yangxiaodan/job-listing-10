<h1> Edit a job </h1>

<%= simple_form_for [:admin, @job ] do |f| %>

<%= f.input :title %>
<%= f.input :description %>



<%= f.input :city, :label => "Requirement" %>
<%= f.input :salary, :label => "Salary" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
  <%= f.submit "Submit" %>
<% end %>
