<h1> Add a job </h1>

<%= simple_form_for @job do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :company, :label => "Company" %>
<%= f.input :city, :label => "City" %>
<%= f.input :salary, :label => "Salary" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
  <%= f.submit "Submit" %>
<% end %>
